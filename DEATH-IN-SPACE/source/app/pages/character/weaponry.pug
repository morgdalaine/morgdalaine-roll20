include ../../components/weaponry/equipment

mixin weaponry_page
  div.page-container
    input(
      type='radio'
      name='attr_character_page'
      class='weaponry hide-element'
      value='4')
    div.weaponry.showhide
      +weapon_block(1)
      +weapon_block(2)
      +armor_block
      +holds_debt
